<workflow>
    <phase>
        <executor>com.atypon.mapreduce.Phases.splitphase.SplitExecutor</executor>
        <rollback>com.atypon.mapreduce.Phases.DummyRollback</rollback>
    </phase>

    <phase>
        <executor>com.atypon.mapreduce.Phases.swarm.buildswarmcompose.SwarmComposeInitExecutor</executor>
        <rollback>com.atypon.mapreduce.Phases.DummyRollback</rollback>
    </phase>

    <phase>
        <executor>com.atypon.mapreduce.Phases.local.composerun.ComposeRunnerExecutor</executor>
        <rollback>com.atypon.mapreduce.Phases.DummyRollback</rollback>
    </phase>

    <phase>
        <executor>com.atypon.mapreduce.Phases.local.runandcollect.RunMapReduceExecutor</executor>
        <rollback>com.atypon.mapreduce.Phases.DummyRollback</rollback>
    </phase>
</workflow>