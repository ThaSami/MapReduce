<workflow>
    <phase>
        <executor>com.atypon.mapreduce.Phases.sharedphases.splitphase.SplitExecutor</executor>
        <rollback>com.atypon.mapreduce.Phases.DummyRollback</rollback>
    </phase>

    <phase>
        <executor>com.atypon.mapreduce.Phases.sharedphases.preparedocker.DockerPrepareExecutor</executor>
        <rollback>com.atypon.mapreduce.Phases.DummyRollback</rollback>
    </phase>

    <phase>
        <executor>com.atypon.mapreduce.Phases.sharedphases.imagebuilder.DockerImageBuilderExecutor</executor>
        <rollback>com.atypon.mapreduce.Phases.DummyRollback</rollback>
    </phase>

    <phase>
        <executor>com.atypon.mapreduce.Phases.local.composerun.DockerComposeExecutor</executor>
        <rollback>com.atypon.mapreduce.Phases.DummyRollback</rollback>
    </phase>

    <phase>
        <executor>com.atypon.mapreduce.Phases.local.runandcollect.RunMapReduceExecutor</executor>
        <rollback>com.atypon.mapreduce.Phases.DummyRollback</rollback>
    </phase>
</workflow>